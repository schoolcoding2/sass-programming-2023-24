import random

number = random.randint(1, 100)
print("Guess a number between 1 and 100 inclusive")
guess = -1
while (guess != number):
    guess = int(input())
    if (guess == number):
        print("Congrats, the number was " + str(guess))
    elif (guess < number):
        print("Higher")
    else:
        print("Lower")
